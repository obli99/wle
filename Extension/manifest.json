{

"name":"Open Multiple Websites",
"description":"Open multiple links easily",
"version":"1.0",

"manifest_version":2,

"browser_action":{
"default_icon":"icon.png",
"default_popup":"popup.html"
},
    
"content_scripts": [{
"matches": ["*//www.amazon.in/*"],
"js": ["script.js"]

}],

"permissions":[
   "tabs",
   "storage"
]

}
